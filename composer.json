{
    "name": "damichixl/import",
    "description": "A Laravel package for importing large amounts of information",
    "type": "package",
    "require": {
        "laravel/framework": "^11.31"
    },
    "autoload": {
        "psr-4": {
            "DamichiXL\\Import\\": "src/",
            "DamichiXL\\Import\\Tests\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "Maksym Sliusarchuk",
            "email": "max.slyusarchuk@gmail.com"
        }
    ],
    "extra": {
        "laravel": {
            "providers" : [
                "DamichiXL\\Import\\Providers\\ImportServiceProvider"
            ]
        }
    },
    "require-dev": {
        "pestphp/pest": "^4.0",
        "pestphp/pest-plugin-laravel": "^4.0",
        "pestphp/pest-plugin-faker": "^4.0",
        "orchestra/testbench": "^9.15"
    },
    "config": {
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    }
}
